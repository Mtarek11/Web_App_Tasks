@using Models;
@using WebApplication;
@model Product;
<div>
    @foreach (var item in Model.ProductAttachments)
    {
        <img src="/Images/@item.Image" class="d-block w-100" alt="...">
    }
    <h2>@Model.Name</h2>
    <h2>@Model.Price</h2>
    <h2>@Model.Category</h2>
    <h2>@Model.Description</h2>
</div>